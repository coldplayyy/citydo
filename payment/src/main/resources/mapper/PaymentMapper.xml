<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.citydo.huzhou.nx.dao.PaymentDao">
    <insert id="create" parameterType="TableList" useGeneratedKeys="true">
        insert into nxq_table_list(departmentName,table_cn_name,table_en_name,resCode,column_cnt)
        values (#{departmentName},#{tableCnName},#{tableEnName},#{resCode},#{columnCnt});
    </insert>

    <resultMap id="paymentResultMap" type="cn.com.citydo.huzhou.nx.entities.TableList">
        <id column="departmentName" property="departmentName" jdbcType="VARCHAR"/>
        <id column="table_cn_name" property="tableCnName" jdbcType="VARCHAR"/>
        <id column="table_en_name" property="tableEnName" jdbcType="VARCHAR"/>
        <id column="resCode" property="resCode" jdbcType="VARCHAR"/>
        <id column="column_cnt" property="columnCnt" jdbcType="BIGINT"/>
    </resultMap>
    <select id="getPaymentByCode" parameterType="String" resultMap="paymentResultMap">
        select departmentname,table_cn_name,table_en_name,resCode,column_cnt from nxq_table_list where resCode=#{resCode};
    </select>
</mapper>